<odoo>
    <record id="view_tour_booking_form" model="ir.ui.view">
        <field name="name">tour.booking.form</field>
        <field name="model">tour.booking</field>
        <field name="inherit_id" ref="base.view_tour_booking_form"/>
        <field name="arch" type="xml">
            <xpath expr="//form/sheet" position="inside">
                <group>
                    <field name="vehicle_id"/>
                </group>
            </xpath>
        </field>
    </record>
</odoo>
